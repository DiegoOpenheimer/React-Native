<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:windowBackground">@android:color/holo_blue_dark</item>
        <item name="colorPrimaryDark">@android:color/holo_blue_dark</item>
        <item name="colorPrimary">@android:color/holo_blue_dark</item>
        <item name="android:navigationBarColor">?attr/colorPrimary</item>
        <item name="android:colorBackground">?attr/colorPrimary</item>
        <item name="colorAccent">@android:color/holo_blue_dark</item>
        <item name="android:colorForeground">@android:color/holo_blue_dark</item>
        <item name="android:textColorPrimaryInverse">@android:color/holo_blue_dark</item>
        <item name="android:textColorSecondaryInverse">@android:color/holo_blue_dark</item>
    </style>

    <style name="Pokemon" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/primary_material_light_1</item>
        <item name="android:statusBarColor">?android:attr/colorControlActivated</item>
        <item name="android:textColorPrimaryInverse">@android:color/white</item>
        <item name="android:windowBackground">@color/primary_material_light_1</item>
        <item name="android:textColorPrimary">@android:color/background_dark</item>
        <item name="android:navigationBarColor">@color/primary_material_light_1</item>
        <item name="android:textColorSecondary">@android:color/white</item>
        <item name="android:textColorSecondaryInverse">@android:color/white</item>
        <item name="android:colorBackground">@android:color/white</item>
    </style>
</resources>